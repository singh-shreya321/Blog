<h3 class="title">
 <strong>Email:</strong>
 <%= @user.email %>
 <br>
 <strong>Name:</strong>
 <%= @user.name %>
 <br>
 <strong>Profile:</strong>
 <%= @user.profile %>
 <br>
</h3>
 <% @posts1.each do |post| %>
  <div class = "post_wrapper">
  <h2 class = "title"><%= link_to post.title, post_path(post) %></h2>
  <p class = "date"><%= post.created_at.strftime("%B, %d, %Y") %></p>
</div>
<% end %>
